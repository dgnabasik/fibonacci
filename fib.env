# Change NODE_ENV to 'production' to run in docker container.
# Change <<PWD>> in FIB_DATABASE_URL to run local Postgres server.
export NODE_ENV=production  # development
export FIB_DATABASE_URL=postgres://postgres:<<PWD>>@localhost:5432/postgres?sslmode=disable
export POSTGRES_USER=postgres
export POSTGRES_PASSWORD=fib
export POSTGRES_DB=postgres
